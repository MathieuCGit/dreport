	--apache2)source "plugins-available/apache2";;
	--aptGet)source "plugins-available/aptGet";;
	--diskUsage)source "plugins-available/diskUsage";;
	--ifconfig)source "plugins-available/ifconfig";;
	--iptables)source "plugins-available/iptables";;
	--lsmod)source "plugins-available/lsmod";;
	--memUsage)source "plugins-available/memUsage";;
	--mircodc2)source "plugins-available/mircodc2";;
	--netstat)source "plugins-available/netstat";;
	--psinfo)source "plugins-available/psinfo";;
	--pstree)source "plugins-available/pstree";;
	--resolv)source "plugins-available/resolv";;
	--route)source "plugins-available/route";;
	--SendByMail)source "plugins-available/SendByMail";;
	--settings)source "plugins-available/settings";;
	--ssh)source "plugins-available/ssh";;
	--uptime)source "plugins-available/uptime";;
	--who)source "plugins-available/who";;
	*)echo "Unknow option, please try dreport -h";exit;;
esac
